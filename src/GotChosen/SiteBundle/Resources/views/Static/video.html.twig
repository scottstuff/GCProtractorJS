    <script>
        $(function() {
            var curTab = "{{ tab }}";
            $('#scholarship-tabs').find('a#tab_' + curTab).tab('show');

            $('a[data-toggle="tab"]').on('shown', function (e) {
                _gaq.push([
                    '_trackPageview',
                    '/{{ app.request.locale }}/video-scholarship/' + e.target.id.replace('tab_', '')
                ]);
            })
        });
    </script>
    
    <script>
        var MAX_MSG = 'You have reached the maximum number of votes for today';

        $(function() {
            var $vote = $('#vote_button');
            $vote.click(function(e) {
                e.preventDefault();
                if ( $vote.hasClass('disabled') ) {
                    if ( $vote.hasClass('maximum-reached') ) {
                        alert(MAX_MSG);
                    }
                    return;
                }

                $vote.addClass('disabled');
                
            });
        });
    </script>    

